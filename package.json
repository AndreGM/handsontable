{
  "name": "handsontable",
  "description": "Handsontable is a JavaScript Spreadsheet Component available for React, Angular and Vue.",
  "homepage": "https://handsontable.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/handsontable/handsontable.git"
  },
  "bugs": {
    "url": "https://github.com/handsontable/handsontable/issues"
  },
  "author": "Handsoncode <hello@handsontable.com>",
  "version": "9.0.2",
  "main": "index",
  "module": "index.mjs",
  "jsnext:main": "index.mjs",
  "jsdelivr": "dist/handsontable.full.min.js",
  "unpkg": "dist/handsontable.full.min.js",
  "scripts": {
    "in": "cross-env-shell COMMAND_ENV=in node scripts/translate-to-native-npm.mjs",
    "all": "cross-env-shell COMMAND_ENV=all node scripts/translate-to-native-npm.mjs",
    "clean:node_modules": "node --experimental-json-modules ./scripts/clean-node-modules.mjs",
    "lint": "npm run eslint && npm run all eslint",
    "eslint": "eslint .github/scripts/ bin/changelog scripts/",
    "test": "npm run all test",
    "build": "npm run all build",
    "freeze": "node --experimental-json-modules handsontable/scripts/freeze.mjs",
    "release": "node --experimental-json-modules handsontable/scripts/release.mjs",
    "changelog": "bin/changelog",
    "publish-all": "npm run all publish-package",
    "prein": "npm run swap-package-links",
    "preall": "npm run swap-package-links",
    "postbuild": "node handsontable/scripts/prepare-package-for-publish.mjs && npm run swap-package-links",
    "swap-package-links": "node --experimental-json-modules ./scripts/swap-package-links.mjs handsontable",
    "examples:version": "node examples/scripts/code-examples.mjs version",
    "examples:install": "node examples/scripts/code-examples.mjs install",
    "examples:build": "node examples/scripts/code-examples.mjs build",
    "examples:test": "node examples/scripts/code-examples.mjs test",
    "examples:start": "http-server ./examples/tmp"
  },
  "keywords": [
    "data",
    "grid",
    "table",
    "editor",
    "grid-editor",
    "data-grid",
    "data-table",
    "spreadsheet",
    "excel",
    "tabular-data",
    "edit-cell",
    "editable-table",
    "data-spreadsheet"
  ],
  "devDependencies": {

  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "workspaces": [
      "handsontable",
      "wrappers/*",
      "examples"
  ]
}
